    <div id="term-header" class="header-info">
        <div id="pinyin"><h2><strong>干净</strong><span><b>[gān jìng]</b><a href="#" url="https://ss0.baidu.com/6KAZsjip0QIZ8tyhnq/text2audio?tex=干净&cuid=dict&lan=ZH&ctp=1&pdt=30&vol=9&spd=4" class="mp3-play">&nbsp;</a></span></h2></div>
                <a href="https://passport.baidu.com/v2/?login&tpl=mn&u=http%3A%2F%2Fdict.baidu.com%2F" id="wordbook" title="登陆">生词本</a>
        <script>window.basicInfo = {"name":"\u5e72\u51c0","definition":"1. \u6ca1\u6709\u5c18\u571f\u3001\u65e0\u6c61\u67d3\u3001\u65e0\u6c61\u57a2\u628a\u9662\u5b50\u626b\u5e72\u51c0\u845b\u6885\u5230\u91cc\u95f4\u5c4b\u6362\u4e86\u4e00\u8eab\u5e72\u51c0\u7684\u84dd\u5e03\u8863\u88f3\u3002\u2014\u2014\u300a\u845b\u6885\u300b 2. \u7206\u70b8\u65f6\u5f88\u5c11(\u6216\u65e0)\u653e\u5c04\u5c18\u57c3\u5e72\u51c0\u6838\u5f39 3. \u5b8c\u5168,\u5f7b\u5e95\u3002\u6bd4\u55bb\u4e1d\u6beb\u4e0d\u5269\u5730\u5fd8\u5e72\u51c0\u4e86\u628a\u654c\u4eba\u6d88\u706d\u5e72\u51c0 4. [\u6587\u8f9e] \u7b80\u6d01,\u4e0d\u62d6\u6ce5\u5e26\u6c34\u6587\u7b14\u5e72\u51c0"};</script>
