    <div id="term-header" class="header-info">
        <div id="pinyin"><h2><strong>实在</strong><span><b>[shí zài]</b><a href="#" url="https://ss0.baidu.com/6KAZsjip0QIZ8tyhnq/text2audio?tex=实在&cuid=dict&lan=ZH&ctp=1&pdt=30&vol=9&spd=4" class="mp3-play">&nbsp;</a></span></h2></div>
                <a href="https://passport.baidu.com/v2/?login&tpl=mn&u=http%3A%2F%2Fdict.baidu.com%2F" id="wordbook" title="登陆">生词本</a>
        <script>window.basicInfo = {"name":"\u5b9e\u5728","definition":"1. \u786e\u5b9e\u5730,\u6beb\u65e0\u7591\u95ee\u5730\u767d\u6768\u6811\u5b9e\u5728\u662f\u4e0d\u5e73\u51e1\u7684,\u6211\u8d5e\u7f8e\u767d\u6768\u6811\u3002--\u300a\u767d\u6768\u793c\u8d5e\u300b 2. \u5b9e\u9645\u4e0a;\u5176\u5b9e\u4ed6\u8bf4\u660e\u767d\u4e86,\u5b9e\u5728\u5e76\u6ca1\u6709\u7a7a\u660e\u767d 3. \u5e72\u5f97\u597d,\u624e\u5b9e;\u5730\u9053\u5de5\u4f5c\u505a\u5f97\u5f88\u5b9e\u5728 4. \u8bda\u5b9e\u4e3a\u4eba\u5b9e\u5728 5. \u771f\u5b9e;\u4e0d\u865a\u5047\u5b9e\u5728\u7684\u672c\u9886\u4f60\u8fd9\u4e2a\u4eba\u5012\u5b9e\u5728,\u53eb\u4f60\u6765\u4f60\u5c31\u6765\u4e86\u3002--\u300a\u5c71\u5730\u56de\u5fc6\u300b"};</script>
